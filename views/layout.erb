
<!doctype html>
<html>
<head>
  <meta charset="utf-8" />
  <link href='http://fonts.googleapis.com/css?family=Quintessential' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Miss+Fajardose' rel='stylesheet' type='text/css'>

  <link rel="stylesheet" href="/lib/bootstrap/css/bootstrap.css" />
  <link rel="stylesheet" href="/css/base.css" type="text/css" />
  <link rel="stylesheet" href="/css/style.css" type="text/css" />

  <script src="/lib/jquery/jquery-1.9.1.min.js"></script>
  </head>
<body>
  <div id="container" class="container-fluid">
   <div class="row topnav front">
      <div>
        <h1><a href="/">TutorialTube</a></h1>
        <h2>Collecting the Best Tutorials on YouTube</h2>
          <div class="navbar navbar-inverse tutorialtube-navbar">
            <div class="navbar-inner">
              <ul class="nav">
                <li><a href="/">Home</a></li>
                <li><a href="/new">New Link</a></li>
                <% @nav_rows.each do |row| %>
                <li><a href="/videos/<%= row['genre'] %>"><%= row['genre'] %></a></li>
                <% end %>
              </ul>
            </div>
          </div>
        </div>
    </div>
    <div class='row-fluid'>
      <div class='span2'>
        <!-- SIDEBAR NAVIGATION -->
          <nav class='sidebar'>
            <ul>
               <% @nav_rows.each do |row| %>
                <li><a href="/videos/<%= row['genre'] %>"><%= row['genre'] %></a></li>
                <% end %>
             </ul>
          </nav>
        <!-- END SIDEBAR NAVIGATION -->
      </div>
      <div class="span10">
        <%= yield %>
      </div>
    </div>



<footer>
  Copyright &copy; 2013 TutorialTube.com
</footer>
</div>
</body>




</html>
